{
 "compileOnSave": true,

 "compilerOptions": {
   "target": "es5",
   "module": "commonjs",
   "moduleResolution": "node",
   "sourceMap": true,
   "emitDecoratorMetadata": true,
   "experimentalDecorators": true,
   "removeComments": true,
   "noImplicitAny": true,
   "skipLibCheck": true
 },
 "lib": [
   "es6",
   "dom"
 ],
 "types": [
   "node"
 ],
 "exclude": [
   "node_modules",
   "typings"
 ]
}